{
  "presets" : [ "@babel/env" ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "env": {
    "release": {
      "presets": [
        ["minify", { "mangle": false }]
      ],
      "plugins": [
        ["conditional-compile", { "define": { "RELEASE": true } }]
      ]
    }
  }
}
