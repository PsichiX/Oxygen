{
  "name": "oxygen-core",
  "version": "1.4.40",
  "description": "Oxygen game engine (Xenon Core for browsers)",
  "main": "bin/index.js",
  "scripts": {
    "clean": "rm -rf bin bin-tools docs",
    "build": "babel src -d bin && node ./versionize.js",
    "build-tools": "babel tools -d bin-tools",
    "build-standalone": "webpack -p",
    "docs": "esdoc -c esdoc.json",
    "make": "npm run clean && npm run build && npm run build-standalone && npm run build-tools && npm run docs",
    "postinstall": "npm run make"
  },
  "bin": {
    "oxy-pack": "./cli/pack.js",
    "oxy-spine": "./cli/spine.js",
    "oxy-tiled": "./cli/tiled.js"
  },
  "files": [
    "assets/",
    "cli/",
    "parsers/",
    "src/",
    "tools/",
    "docs/",
    ".babelrc",
    "esdoc.json",
    "versionize.js",
    "bin/version.json",
    "bin/oxygen-core.js",
    "bin/oxygen-core.js.map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PsichiX/Oxygen.git"
  },
  "keywords": [
    "webgl",
    "browser",
    "game",
    "engine",
    "es7"
  ],
  "author": "Patryk 'PsichiX' Budzynski <psichix@gmail.com> (http://psichix.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PsichiX/Oxygen/issues"
  },
  "homepage": "https://github.com/PsichiX/Oxygen#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "babel-loader": "^8.0.4",
    "bezier-easing": "^2.1.0",
    "box2dweb-commonjs": "^2.1.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "stream-buffers": "^3.0.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "xml2js": "^0.4.19"
  }
}
