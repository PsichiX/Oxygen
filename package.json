{
  "name": "oxygen-core",
  "version": "1.3.24",
  "description": "Oxygen game engine (Xenon Core for browsers)",
  "main": "bin/index.js",
  "scripts": {
    "clean": "rm -rf bin bin-tools docs",
    "build": "./node_modules/.bin/babel src -d bin && node ./versionize.js",
    "build-tools": "./node_modules/.bin/babel tools -d bin-tools",
    "browserify": "./node_modules/.bin/browserify bin/index.js -o bin/oxygen-core.js -s Oxygen",
    "browserify-uglify": "./node_modules/.bin/uglifyjs -nm -o bin/oxygen-core.min.js bin/oxygen-core.js",
    "browserify-min": "npm run browserify && npm run browserify-uglify",
    "docs": "esdoc -c esdoc.json",
    "make": "npm run clean && npm run build && npm run build-tools && npm run browserify-min && npm run docs",
    "postinstall": "npm run make",
    "oxygen-pack": "node ./bin-tools/pack-cli.js",
    "oxygen-spine": "node ./bin-tools/spine-cli.js"
  },
  "bin": {
    "oxy-pack": "./cli/pack.js",
    "oxy-spine": "./cli/spine.js",
    "oxy-tiled": "./cli/tiled.js"
  },
  "files": [
    "assets/",
    "cli/",
    "parsers/",
    "src/",
    "tools/",
    "docs/",
    ".babelrc",
    "esdoc.json",
    "versionize.js",
    "bin/oxygen-core.js",
    "bin/oxygen-core.min.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PsichiX/Oxygen.git"
  },
  "keywords": [
    "webgl",
    "browser",
    "game",
    "engine",
    "es7"
  ],
  "author": "Patryk 'PsichiX' Budzynski <psichix@gmail.com> (http://psichix.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PsichiX/Oxygen/issues"
  },
  "homepage": "https://github.com/PsichiX/Oxygen#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-conditional-compile": "0.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "bezier-easing": "^2.0.3",
    "box2dweb-commonjs": "^2.1.0",
    "browserify": "^15.2.0",
    "esdoc": "^1.0.4",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "leapjs": "^0.6.4",
    "stream-buffers": "^3.0.1",
    "uglify-js": "^3.3.12",
    "xml2js": "^0.4.19"
  }
}
